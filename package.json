    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "*",
    "sinon": "^2.2.0",
    "source-map-support": "0.4.14",
    "stream-to-string": "1.1.0",
    "three": "^0.85.2",
    "vinyl-source-stream": "^1.1.0",
    "watch": "*",
    "webpack": "^2.6.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "orbit-controls": "^1.1.3",
    "react": "^15.5.4",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.5.4",
    "stats.js": "^0.17.0",
    "three": "^0.85.2"
  }
}
